generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  nombre    String?
  apellido  String?
  telefono  String?
  direccion String?
  fotoPerfil String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
